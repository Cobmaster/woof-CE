if [ -e lib/firmware/regulatory.db-upstream && ! -e lib/firmware/regulatory.db ]; then
	ln -s regulatory.db-upstream lib/firmware/regulatory.db
fi

if [ -e lib/firmware/regulatory.db.p7s-upstream && ! -e lib/firmware/regulatory.db.p7s ]; then
	ln -s regulatory.db.p7s-upstream lib/firmware/regulatory.db.p7s-upstream
fi